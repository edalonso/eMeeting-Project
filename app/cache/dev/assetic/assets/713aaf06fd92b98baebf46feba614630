function filter_emeetings(a){$("#dropdown-title").html(a);if(a=="All eMeetings"){$(".ui-state-default div").fadeIn("slow").removeClass("hidden")}else{$(".ui-state-default > div").each(function(){if(!$(this).hasClass(a)){console.log(this);$(this).fadeOut("normal").addClass("hidden")}else{console.log(this);$(this).fadeIn("slow").removeClass("hidden")}})}};